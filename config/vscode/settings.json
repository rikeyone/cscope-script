        "fs/udf": true,
        "fs/host_fs": true,
        "scripts/dtc": true,
        "LICENSES": true,
        ".clang-format": true,
        ".cocciconfig": true,
        ".get_maintainer.ignore": true,
        ".gitattributes": true,
        ".gitlab-ci.yml": true,
        ".mailmap": true,
        "*.code-workspace": true,
        "config.aarch64": true,
        "*.patch": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },
    "highlight.regexes": {
        "(SELinux).*:": {
            "filterFileRegex": ".*\\.(log|txt)",
            "decorations": [
                { "color": "#ff00ff" },
            ],
        },
        "(Call trace):": {
            "filterFileRegex": ".*\\.(log|txt)",
            "decorations": [
                { "color": "#ff0000" },
            ],
        },
        "(charger)": {
            "filterFileRegex": ".*\\.(log|txt)",
            "decorations": [
                { "color": "#ff0000" },
            ],
        },
    },
    "highlight.maxMatches": 100000,
    "C_Cpp.dimInactiveRegions":false,
    "editor.fontFamily": "'JetBrains Mono','JetBrains Mono ExtraBold','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'"
}